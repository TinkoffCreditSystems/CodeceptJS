{
  "extends": "dtslint/dtslint.json",
  "rules": {
    "semicolon": false,
    "no-empty-interface": false,
    "interface-name": false,
    "jsdoc-format": false,
    "max-line-length": false,
    "no-single-declare-module": false
  },
  "linterOptions": {
    "exclude": [
      "./types.d.ts"
    ]
  }
}
